{
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"jsxBracketSameLine": true,
	"overrides": [
		{
			"files": "*.js",
			"options": {
				"parser": "babylon"
			}
		},
		{
			"files": "*.jsx",
			"options": {
				"parser": "babylon"
			}
		},
		{
			"files": "*.scss",
			"options": {
				"parser": "scss"
			}
		}
	]
}